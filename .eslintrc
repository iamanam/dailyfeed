{
    "extends": [
        "plugin:react/recommended",
        "standard",
        "prettier",
        "prettier/react"
    ],
    "plugins": [
        "prettier"
    ],
    "env": {
        "es6": true,
        "node": true
    },
    "rules": {
        "prettier/prettier": [
            "error",
            {}
        ]
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    }
}